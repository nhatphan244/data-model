# Cast the 'ward_id' column to integer, safely handling NaN as NULL
gov_master_df['ward_id'] = pd.to_numeric(gov_master_df['ward_id'], errors='coerce').astype('Int64')  # 'Int64' (capital I) supports NaN
ward_df['ward_id'] = pd.to_numeric(ward_df['ward_id'], errors='coerce').astype('Int64')

# Now, use to_sql to push to the database
gov_master_df.to_sql('gov_master', con=engine, dtype=column_data_types, index=False, if_exists='replace')
ward_df.to_sql('ward', con=engine, dtype=column_data_types, index=False, if_exists='replace')
